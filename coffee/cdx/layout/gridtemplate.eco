
<% for e in @elements: %>
<div data-row="<%= e.r %>"
     data-col="<%= e.c %>"
     data-sizex="<%= e.x %>"
     data-sizey="<%= e.y %>"
     class="griditem">
    <strong ><%= e.title %></strong>
    <div id="<%= e.title %>"
         class="<%= e.title %> griditem-content">
    </div>
</div>
<% end %>
